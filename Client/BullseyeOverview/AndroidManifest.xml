<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.sync.bullseye"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.sync.bullseye.BullseyeOverview"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.sync.bullseye.AccountOptions"
            android:label="Account Options" >
        </activity>
        <activity
            android:name="org.sync.bullseye.NewGame"
            android:label="Account Options" >
        </activity>
        <activity
            android:name="org.sync.bullseye.StoreDisplay"
            android:label="Account Options" >
        </activity>
    </application>

</manifest>
